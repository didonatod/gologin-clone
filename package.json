{
  "name": "ticketpro",
  "version": "1.0.0",
  "description": "TicketPro - Professional Browser Profile Management",
  "main": "main.js",
  "scripts": {
    "start-react": "cross-env BROWSER=none react-app-rewired start",
    "start-electron": "electron .",
    "dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && npx nodemon --watch main.js --watch preload.js --exec electron .\"",
    "restart": "npm run dev",
    "build": "react-app-rewired build",
    "postbuild": "mkdir -p build/icons/notifications && cp public/icons/notifications/*.png build/icons/notifications/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "electron .",
    "electron-build": "electron-builder",
    "start:react": "react-scripts start",
    "start:electron": "wait-on tcp:3000 && electron .",
    "test-purchaser": "node tests/ticketPurchaser.test.js",
    "verify-changes": "npm run test-purchaser"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hello-pangea/dnd": "^16.0.1",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.14.18",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "electron": "^34.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.4.3",
    "html-to-image": "^1.11.13",
    "puppeteer-core": "^24.2.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "recharts": "^2.15.1",
    "redux": "^4.2.1",
    "seedrandom": "^3.0.5",
    "socks-proxy-agent": "^8.0.5",
    "ua-parser-js": "^2.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@jest/globals": "^29.7.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "assert": "^2.1.0",
    "babel-jest": "^29.7.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "eventemitter3": "^5.0.1",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.22",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "puppeteer": "^21.0.0",
    "react-app-rewired": "^2.2.1",
    "react-refresh": "^0.16.0",
    "react-scripts": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "wait-on": "^8.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "build/icons/notifications",
        "to": "icons/notifications",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
